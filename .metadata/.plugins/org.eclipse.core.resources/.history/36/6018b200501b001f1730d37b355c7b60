package data;

public class App 
{
    public static void main( String[] args )
    {
        Professor professor=new Professor("Dr. Rohan",31,"Mech");
        professor.addCourse("DS");
        professor.addCourse("ML");
        professor.addCourse("Algorithms");
        
        System.out.print("Initial Courses: ");
        professor.displayCourses();
        
        professor.addCourse("AI");
        professor.deleteCourse("ML");
        System.out.print("After Changing the courses:");
        professor.displayCourses();
        
    }
}
